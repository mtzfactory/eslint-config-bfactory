    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
